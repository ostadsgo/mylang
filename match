// You can't have body in match condition you have to define a function if you need to run more than one line

let age = 20;

let result = match age {
	in range(1, 13) => "Kid";
	in range(13, 20) => "Teeniger";
	in range(20, 40) => "Yong";
	in range(30, 60) => "Old";
	in range(60, 90) => "Very old";
	in range(90, int.maxvalue) => "Very very old";
	else => "Invlid age!";
}






